{
  "name": "horimisli.me",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "<10.0.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next-remote-watch ./posts",
    "prod": "npm run gen-feed && npm run gen-sitemap && next build && next export",
    "gen-feed": "ts-node -r dotenv/config scripts/feed.ts",
    "gen-sitemap": "ts-node -r dotenv/config scripts/sitemap.ts",
    "import": "ts-node -r dotenv/config scripts/bear_note_importer.ts",
    "push-entries": "cd posts;git add .;git commit -m 'add post'; git p; cd ../public/images; git add .; git commit -m 'add post'; git p; cd ../..",
    "release": "gh workflow run release.yml",
    "release-watch": "npm run release && gh run list --workflow=release.yml --json=databaseId --limit=1 --jq '.[].databaseId' | xargs -IJOB_ID gh run watch JOB_ID"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "adm-zip": "0.5.10",
    "date-fns": "2.29.3",
    "date-fns-timezone": "0.1.4",
    "dotenv": "16.0.3",
    "gray-matter": "4.0.3",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "luxon": "3.3.0",
    "next": "13.2.4",
    "next-compose-plugins": "2.2.1",
    "next-optimized-images": "2.6.2",
    "next-remote-watch": "2.0.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-emoji-render": "2.0.1",
    "react-markdown": "8.0.6",
    "react-syntax-highlighter": "15.5.0",
    "rehype-raw": "6.1.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-prism": "1.3.6",
    "remark-unwrap-images": "3.0.1",
    "rss": "1.2.2"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.0",
    "@types/luxon": "3.2.0",
    "@types/node": "18.15.7",
    "@types/prismjs": "1.26.0",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "15.5.6",
    "@types/remark-prism": "1.3.4",
    "@types/rss": "0.0.30",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "postcss": "8.4.21",
    "tailwindcss": "3.2.7",
    "textlint": "13.3.1",
    "textlint-rule-preset-ja-spacing": "2.3.0",
    "textlint-rule-preset-ja-technical-writing": "7.0.0",
    "textlint-rule-spellcheck-tech-word": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  }
}
