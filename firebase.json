{
  "hosting": {
    "public": "out",
    "ignore": [
      "images/**"
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "404.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      }
    ]
  }
}
